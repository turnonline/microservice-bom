                <version>2.2.2</version>
            </dependency>
            <dependency>
                <groupId>com.google.endpoints</groupId>
                <artifactId>endpoints-management-control-all</artifactId>
                <version>1.0.14</version>
            </dependency>
            <dependency>
                <groupId>com.google.endpoints</groupId>
