<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <parent>
        <groupId>org.ctoolkit.maven</groupId>
        <artifactId>ctoolkit-public</artifactId>
        <version>4</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>
    <artifactId>ctoolkit-microservice-bom</artifactId>
    <version>2.0.22</version>
    <packaging>pom</packaging>
    <name>TurnOnline.biz microservice on the Google App Engine standard for Java 11+ that provides access to legacy services</name>
    <description>
        TurnOnline.biz microservice based on the Google App Engine standard for Java x11+, providing access to legacy bundled services.
        Bill of Materials: This component keeps track of dependency versions and ensures that everything works well together.
    </description>
    <url>https://github.com/turnonline/ctoolkit-microservice-bom</url>

    <scm>
        <connection>scm:git:[fetch=]git@github.com:turnonline/ctoolkit-microservice-bom.git</connection>
        <developerConnection>scm:git:[push=]git@github.com:turnonline/ctoolkit-microservice-bom.git
        </developerConnection>
        <url>https://github.com/turnonline/ctoolkit-microservice-bom</url>
        <tag>ctoolkit-microservice-bom-2.0.22</tag>
    </scm>

    <issueManagement>
        <url>https://github.com/turnonline/ctoolkit-microservice-bom/issues</url>
    </issueManagement>

    <developers>
        <developer>
            <name>Aurel Medvegy</name>
            <id>medvegy</id>
            <email>medvegy@turnonline.biz</email>
        </developer>
    </developers>

    <licenses>
        <license>
            <name>Apache License, Version 2.0</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
            <distribution>repo</distribution>
        </license>
    </licenses>

    <!-- Use here only a release version of dependency -->
    <dependencyManagement>
        <dependencies>
            <!-- AppEngine dependencies -->
            <dependency>
                <groupId>com.google.appengine</groupId>
                <artifactId>appengine-api-1.0-sdk</artifactId>
                <version>2.0.22</version>
            </dependency>
            <dependency>
                <groupId>com.google.appengine</groupId>
                <artifactId>appengine-api-legacy</artifactId>
                <version>2.0.22</version>
            </dependency>
            <dependency>
                <groupId>com.google.appengine</groupId>
                <artifactId>appengine-endpoints</artifactId>
                <version>1.9.86</version>
            </dependency>
            <dependency>
                <groupId>com.google.appengine</groupId>
                <artifactId>appengine-local-endpoints</artifactId>
                <version>1.9.86</version>
            </dependency>
            <dependency>
                <groupId>com.google.appengine</groupId>
                <artifactId>appengine-remote-api</artifactId>
                <version>2.0.22</version>
            </dependency>
            <dependency>
                <groupId>com.google.appengine</groupId>
                <artifactId>appengine-tools-sdk</artifactId>
                <version>2.0.22</version>
            </dependency>
            <!-- AppEngine dependencies usually as test scope -->
            <dependency>
                <groupId>com.google.appengine</groupId>
                <artifactId>appengine-testing</artifactId>
                <version>2.0.22</version>
            </dependency>
            <dependency>
                <groupId>com.google.appengine</groupId>
                <artifactId>appengine-api-stubs</artifactId>
